createArray

  function ifFunc(expression, ifTrue, ifFalse) {
    if (expression()) {
      ifTrue()
    } else {
      ifFalse();
    }
  }
  function createArray(){
    return arguments;
  }

let x=5;let y=6;ifFunc(function () {return(x>y);},function () {console.log(x)},function () {console.log(y)})console.log('word')
